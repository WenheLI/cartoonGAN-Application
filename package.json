{
  "devDependencies": {
    "parcel": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "sass": "^1.23.7",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@tensorflow-models/body-pix": "^2.0.3",
    "@tensorflow/tfjs": "^1.3.2",
    "dropzone": "^5.5.1",
    "velocity-animate": "^2.0.5"
  },
  "scripts": {
    "start": "npx parcel serve index.html",
    "build": "rm -rf public/* && mkdir public && npx parcel build index.html && mv dist/* public/ && rm -rf dist"
  },
  "staticFiles": {
    "staticPath": "static",
    "watcherGlob": "**"
  }
}
